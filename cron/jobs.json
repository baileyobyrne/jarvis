{
  "version": 1,
  "jobs": [
    {
      "name": "willoughby-monitor-morning",
      "schedule": "0 21 * * *",
      "command": "node /root/.openclaw/skills/agentbox-willoughby/monitor.js",
      "enabled": true
    },
    {
      "name": "willoughby-monitor-midday",
      "schedule": "0 2 * * *",
      "command": "node /root/.openclaw/skills/agentbox-willoughby/monitor.js",
      "enabled": true
    },
    {
      "name": "willoughby-monitor-afternoon",
      "schedule": "0 7 * * *",
      "command": "node /root/.openclaw/skills/agentbox-willoughby/monitor.js",
      "enabled": true
    },
    {
      "name": "willoughby-contacts-refresh",
      "schedule": "0 20 * * *",
      "command": "node /root/.openclaw/skills/agentbox-willoughby/fetch-contacts.js",
      "enabled": true
    },
    {
      "name": "daily-planner-morning",
      "schedule": "0 20 * * 1-5",
      "command": "cd /root/.openclaw/workspace && node daily-planner.js >> /root/.openclaw/workspace/daily-planner.log 2>&1",
      "enabled": true
    },
  ]
}
